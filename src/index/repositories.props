<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="14.0" DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <Repository Include="runtime">
      <Url>https://github.com/dotnet/runtime</Url>
      <Projects>
        src/coreclr/src/System.Private.CoreLib/System.Private.CoreLib.csproj;
        src/libraries/*/src/*.csproj;
        src/libraries/*/src/*.vbproj;
      </Projects>
      <SparseCheckout>
!src/installer/*
!*.ilproj
!*.il
!*.c
!*.h.in
!*.cpp
!*.hpp
!*.inl
!*.asm
!*.S
!*.inc
!*.rc
!docs/*
!CMakeLists.txt
!*.cmake
!*.lst
/*
      </SparseCheckout>
      <PrepareCommand>
        build.cmd -subset clr.corelib+libs.depprojs+libs.ref+libs.src -binarylog -os Linux
      </PrepareCommand>
    </Repository>
    <Repository Include="winforms">
      <Url>https://github.com/dotnet/winforms</Url>
      <Projects>
        src/*/src/*.csproj;
      </Projects>
      <ExcludeProjects>
        src/*/ref/*.csproj;
      </ExcludeProjects>
      <PrepareCommand>
        build
      </PrepareCommand>
    </Repository>
    <Repository Include="wpf">
      <Url>https://github.com/dotnet/wpf</Url>
      <Projects>
        src/Microsoft.DotNet.Wpf/src/*/*.csproj
      </Projects>
      <PrepareCommand>
        build
      </PrepareCommand>
    </Repository>
    <Repository Include="aspnetcore">
      <Url>https://github.com/dotnet/aspnetcore</Url>
      <Projects>
        src/**/src/**/*.csproj;
      </Projects>
      <PrepareCommand>
        restore.cmd -nobuildnodejs
      </PrepareCommand>
    </Repository>
    <Repository Include="dotnet-extensions">
      <Url>https://github.com/dotnet/extensions</Url>
      <Projects>
        src/**/src/**/*.csproj;
      </Projects>
      <PrepareCommand>
        restore.cmd
      </PrepareCommand>
    </Repository>
  </ItemGroup>
</Project>
